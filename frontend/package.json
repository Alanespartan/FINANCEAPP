{
    "name": "finance-frontend",
    "version": "1.0.0",
    "description": "Finance personal web app",
    "private": true,
    "scripts": {
        "watch": "vite build --watch",
        "dev": "vite",
        "build": "run-p type-check build-only",
        "preview": "vite preview --port 4173",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "lint:nofix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
        "restart": "rd /s /q dist node_modules .cache>nul 2>&1|echo.>nul && npm install && npm run build",
        "clean": "rimraf ./dist"
    },
    "author": "Arturo Cruz",
    "contributors": [],
    "license": "UNLICENSED",
    "dependencies": {
        "@popperjs/core": "^2.11.0",
        "@vueform/multiselect": "^2.5.2",
        "@vuepic/vue-datepicker": "^3.2.2",
        "axios": ">=0.28.0",
        "bootstrap": "^5.1.3",
        "chart.js": "^3.8.0",
        "primevue": "^3.46.0",
        "sweetalert2": "11.4.8",
        "uuid": "^8.3.2",
        "vue": "^3.2.37",
        "vue-chartjs": "^4.1.1",
        "vue-css-donut-chart": "^2.0.0",
        "vue-router": "4.3.0",
        "vue-select": "^4.0.0-beta.3",
        "vuex": "^4.0.2",
        "vuex-persist": "^3.1.3"
    },
    "devDependencies": {
        "@babel/plugin-proposal-decorators": "^7.18.2",
        "@rushstack/eslint-patch": "^1.1.0",
        "@types/node": "^16.11.41",
        "@types/uuid": "^8.3.4",
        "@types/vue-select": "^3.16.0",
        "@vitejs/plugin-vue": "^2.3.3",
        "@vue/eslint-config-typescript": "^11.0.0",
        "@vue/tsconfig": "^0.1.3",
        "eslint": "^8.5.0",
        "eslint-plugin-editorconfig": "^3.2.0",
        "eslint-plugin-vue": "^9.0.0",
        "npm-run-all": "^4.1.5",
        "pug": "^3.0.2",
        "sass": "^1.53.0",
        "typescript": "~4.7.4",
        "vite": "^2.9.12",
        "vue-class-component": "^8.0.0-rc.1",
        "vue-tsc": "^0.38.1"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}
